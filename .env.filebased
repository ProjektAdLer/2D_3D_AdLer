# File-Based Backend Configuration
# This mode loads learning worlds from public/LearningWorlds/ directory
# Use extractLearningWorlds.js script to prepare MBZ files
# Auto-login enabled, no Moodle integration

REACT_APP_IS_DEBUG=false
REACT_APP_LOGLEVEL="log"

# File-based backend mode
REACT_APP_USE_FAKEBACKEND=false
REACT_APP_USE_FILEBASED_BACKEND=true

# API URLs (not used in file-based mode, but required for config)
REACT_APP_API_SERVER_URL=http://localhost:3000
REACT_APP_MOODLE_SERVER_URL=http://localhost:3000

CI=false
GENERATE_SOURCEMAP=false
